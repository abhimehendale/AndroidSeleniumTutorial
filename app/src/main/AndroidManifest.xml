<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abhishekmehendale.seleniumwebdriverturorial">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OptionsActivity"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Intro_activity"
            android:label="@string/title_activity_intro_activity"
            android:parentActivityName=".OptionsActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".BasicCommands"
            android:label="@string/title_activity_basic_commands"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Installation"
            android:label="@string/title_activity_installation"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MidCommands"
            android:label="@string/title_activity_mid_commands"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".advCommands"
            android:label="@string/title_activity_adv_commands"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>